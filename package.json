{
  "name": "react-template-libraries",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build --stream",
    "link": "lerna exec -- npm i && lerna link",
    "react-common-components:pack": "cd packages/react-common-components && npm yarn",
    "lint": "lerna run lint --stream",
    "lint:fix": "lerna run lint:fix --stream",
    "copy-img-assets": "cp -r packages/react-common-components/src/assets/img/ storybook-static/",
    "storybook": "start-storybook -s packages/react-common-components/src/assets/ -p 6006",
    "storybook:build": "build-storybook && yarn run copy-img-assets",
    "storybook:serve": "yarn run storybook:build && npx http-server storybook-static"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@storybook/addon-a11y": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addon-viewport": "^6.2.8",
    "@storybook/addons": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react-hooks": "^6.0.0",
    "@types/chart.js": "^2.9.32",
    "@types/color": "^3.0.2",
    "@types/dompurify": "^2.2.2",
    "@types/enzyme": "^3.10.8",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.15",
    "@types/tinymce": "^4.6.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "enzyme": "^3.11.0",
    "eslint": "^7.28.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "lerna": "^4.0.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-styles": "^3.14.1",
    "sass-lint": "^1.13.1",
    "sass-loader": "^10.1.1",
    "size-limit": "^4.10.2",
    "style-loader": "^2.0.0",
    "tsdx": "^0.14.1",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^5.0.1",
    "@tinymce/tinymce-react": "^3.12.5",
    "bootstrap": "^4.6.0",
    "chart.js": "^2.9.4",
    "color": "^3.1.3",
