    "nanoid": "^3.1.23",
    "react-bootstrap": "^1.5.2",
    "react-datetime": "^3.0.4",
    "react-dropzone-uploader": "^2.11.0",
    "react-helmet": "^6.1.0",
    "react-multi-carousel": "^2.6.3",
    "react-player": "^2.9.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.1",
    "tinymce": "^5.8.1",
    "tslib": "^2.3.0"
  }
}
